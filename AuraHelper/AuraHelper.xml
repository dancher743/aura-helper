	</Frame>
	<Frame name="GameTooltip" parent="GameTooltip">
		<Scripts>
			<OnLoad>
			AuraHelper_GameTooltip_OnLoad();
			</OnLoad>
		</Scripts>
	</Frame>
</Ui>
